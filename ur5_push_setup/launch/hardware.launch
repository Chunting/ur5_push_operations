<launch>
	  <include file="$(find tams_ur5_bringup)/launch/tams_ur5_drivers.launch">
    		<arg name="gripper_mode" value="wide_pinch" />
  	</include>


  	<arg name="joint_ranges_config" default="$(find tams_ur5_description)/config/joint_ranges/elbow_up.yaml" />
  	<arg name="floating_table" default="false" />
  	<arg name="r200" default="true" />

  	<param name="robot_description" command="$(find xacro)/xacro.py '$(find ur5_push_setup)/urdf/tams_ur5_setup.urdf.xacro' joint_ranges_config:=$(arg joint_ranges_config) floating_table:=$(arg floating_table) r200:=$(arg r200)" />


	<include file="$(find ur5_push_setup)/launch/move_group.launch"/>

	<include file="$(find object_recognition)/launch/object_recognition.launch">
		<arg name="demo_mode" value="true"/>
	</include>
</launch>
